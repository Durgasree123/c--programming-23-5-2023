#include<stdio.h>
main()
{
int age,i=0,babyage=0,schoolage=0,adultage=0;
while(i<=20)
{
printf("enter age of a person \n");
scanf("%d",&age);
if(age>=0&&age<=5)
babyage++;
else if(age>=6&&age<=17)
schoolage++;
else 
adultage++;
i++;
}
printf("Baby age= %d\n",babyage);
printf("School age= %d\n",schoolage);
printf("Adult age= %d\n",adultage);
}
